{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": ["dist/**"]
    },
    "core#build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": ["internal/**"]
    },
    "docs#build": {
      "dependsOn": [
        "^build",
        "$NEXT_PUBLIC_STRIPE_PUBLIC_KEY",
        "$NEXT_PUBLIC_ANALYTICS_ID"
      ],
      "outputs": [".next/**"]
    }
  },
  "baseBranch": "origin/master",
  "globalDependencies": [
//    "$GITHUB_TOKEN",
    "tsconfig.json"
  ]
}
